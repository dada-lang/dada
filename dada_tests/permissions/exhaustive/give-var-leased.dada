class Pair(a, b)

async fn main() {
    let p = Pair(22, 44).lease
    let q = p.give                    # Giving a leased thing: subleases

    # Accessing `q`: ok
    print(q).await #! OUTPUT Pair\(22, 44\)

    # Accessing `p`: error because `q` still live
    print(p).await #! RUN ERROR cannot access leased data

    # `q` live because of potential use here
    print(q).await
}