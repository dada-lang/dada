class Pair(a, b)

async fn main() {
    let p = Pair(22, 44).lease

    # Sharing a leased thing: creates a shared sublease
    let q = p.share

    # Accessing `q`: ok
    print(q).await #! OUTPUT Pair\(22, 44\)

    # Accessing `p`: error because `q` still live
    print(p.give).await #! RUN ERROR cannot write to shared data

    print(q).await
}